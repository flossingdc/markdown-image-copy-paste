{
    "name": "markdown-image-copy-paste",
    "version": "0.0.0",
    "main": "./lib/markdown-image-copy-paste",
    "description": "Paste image into markdown file.",
    "keywords": [
        "markdown",
        "md",
        "image"
    ],
    "activationCommands": {
        "atom-text-editor": "markdown-image-copy-paste:paste"
    },
    "configSchema": {
        "imagePath": {
            "type": "string",
            "default": "",
            "title": "Save images in this path",
            "description": "The image is saved in the same directory as the md file by default. You can use template strings such as ${fileName} ${fileNameWithoutSuffix}.",
            "order": 0
        }
    },
    "repository": "https://github.com/flossingdc/markdown-image-copy-paste",
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "dependencies": {},
    "deprecated": false
}
